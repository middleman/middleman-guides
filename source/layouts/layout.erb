
<% wrap_layout :base do %>
  <header id="header">
    <div class="container">
      <h1><%= link_to "Middleman", "/", {class: 'logo'} %></h1>
      <p class="console">$ gem install middleman</p>
     
    </div>
  </header>


  <div class="container">
    <nav id="tos">

      <%= partial 'layouts/nav' %>
      
      <footer>
        <a href="http://www.pledgie.com/campaigns/15807">
          <img src="http://www.pledgie.com/campaigns/15807.png" alt="Click here to lend your support to Middleman">
        </a>
      </footer>
          

    </nav>
    <article id="main" role="main">
      <%= yield %>
    </article>
  </div>
<% end %>
